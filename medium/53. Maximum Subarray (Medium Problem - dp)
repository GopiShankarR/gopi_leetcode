class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        n = len(nums)

        if n == 0:
            return 0

        S = [0] * n
        S[0] = nums[0]
        for i in range(1, n):
            if(S[i - 1] < 0):
                S[i] = nums[i]
            else:
                S[i] = S[i - 1] + nums[i]
        
        maxNum = S[0]
        for i in range(1, n):
            if S[i] > maxNum:
                maxNum = S[i]
        
        return maxNum

class Solution:
    def numberOfSubstrings(self, s: str) -> int:
        n = len(s)
        zeroesArray = [n] * (n)
        
        for i in range(n - 2, -1, -1):
            if s[i + 1] == "0":
                zeroesArray[i] = i + 1
            else:
                zeroesArray[i] = zeroesArray[i + 1]

        result = 0

        for l in range(n):
            zeroes = 1 if s[l] == "0" else 0
            r = l

            while zeroes * zeroes <= n:
                nextZ = zeroesArray[r] if r < n else n
                ones = (nextZ - l) - zeroes
                if ones >= zeroes * zeroes:
                    result += min(nextZ - r, ones - zeroes * zeroes + 1)
                
                r = nextZ
                zeroes += 1

                if r == n:
                    break

        return result

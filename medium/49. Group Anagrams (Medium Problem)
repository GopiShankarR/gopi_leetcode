class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        result = []
        hashMap = {}
        for word in strs:
            string = "".join(sorted(word))
            hashMap.setdefault(string, []).append(word)

        for key, val in hashMap.items():
            result.append(val)

        return result

class Solution:
    def maxOperations(self, s: str) -> int:
        countOne = 0
        result = 0
        i = 0
        n = len(s)

        while i < len(s):
            if s[i] == "1":
                countOne += 1
            else:
                while i + 1 < n and s[i + 1] == "0":
                    i += 1
                result += countOne
            i += 1
        
        return result
